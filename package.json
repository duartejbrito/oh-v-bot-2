{
  "name": "oh-v-bot-2",
  "version": "1.0.0",
  "description": "A Discord bot with help features for Once Human.",
  "author": {
    "email": "sytertzp@gmail.com",
    "name": "Duarte 'SyTeR' Brito"
  },
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/duartejbrito/oh-v-bot-2",
  "repository": {
    "type": "git",
    "url": "https://github.com/duartejbrito/oh-v-bot-2.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev-translate": "tsx watch src/language/auto-translate.ts",
    "dev-docker": "npm run build && docker compose down && docker compose --env-file .env.local up -d",
    "translate": "tsx src/language/auto-translate.ts",
    "format": "prettier --write \"**/*.{json,ts}\"",
    "lint": "eslint . --ext ts --fix",
    "build": "tsup src/index.ts --minify",
    "start": "node ."
  },
  "dependencies": {
    "@iamtraction/google-translate": "^2.0.1",
    "colors": "^1.4.0",
    "cron-parser": "^4.9.0",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^11.0.6",
    "node-schedule": "^2.1.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@swc/core": "^1.7.26",
    "@types/node": "^18.19.50",
    "@types/node-schedule": "^2.1.7",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
